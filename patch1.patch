 async function loadHeaderAndFooter() {
     try {
         // Check if we're running from file:// protocol
         if (window.location.protocol === 'file:') {
             console.warn('Running from file:// protocol, using fallback navigation');
             showFallbackNavigation();
             showFallbackFooter();
             return;
         }
         
