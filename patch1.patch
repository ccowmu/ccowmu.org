+    // Calculate days until next Thursday
+    let daysUntilThursday;
+    if (currentDay <= 4) {
+        daysUntilThursday = 4 - currentDay;
+    } else {
+        daysUntilThursday = 7 - currentDay + 4;
+    }
+    
+    // If it's Thursday and after 6 PM, get next Thursday
+    if (currentDay === 4 && today.getHours() >= 18) {
+        daysUntilThursday = 7;
+    }
